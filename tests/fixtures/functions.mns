// Function definitions and calls
greet = function(name)
	print "Hello, " + name + "!"
	return "Greeting sent to " + name
end function

calculate = function(a, b)
	result = a * b + 10
	print "Calculation result: " + result
	return result
end function

// Test function calls
greet "World"
value = calculate(5, 3)
print "Function returned: " + value
