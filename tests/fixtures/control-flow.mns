// If statements and loops
x = 10
if x > 5 then
	print "x is greater than 5"
	print "Let's do some calculations"
	y = x * 2
	print "y = " + y
end if

// While loop with break
counter = 0
while true
	print "Counter is: " + counter
	counter = counter + 1
	if counter >= 3 then break
end while
print "Loop finished!"
