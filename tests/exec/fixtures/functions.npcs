
// Function definitions and calls
greet = function(name)
	print "Hello, " + name + "!"
end function

shake = function(what)
	print "*Shake " + what + "*"
end function

 function calculate(a, b)
	result = a * b + 10
	print "Calculation result: " + result
	return result
end function

// Test function calls
greet "World"
shake "hands"
value = calculate(5, 3)
print "Function returned: " + value
